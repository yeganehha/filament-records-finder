{
    "name": "yeganehha/filament-records-finder",
    "description": "This packages allows you to easily integrate a beautiful Record Finder into your Filament forms.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "filament/forms": "^3.0",
        "filament/tables": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yeganehha\\FilamentRecordsFinder\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Erfan Ebrahimi",
            "email": "me@erfanebrahimi.ir"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Yeganehha\\FilamentRecordsFinder\\FilamentRecordsFinderServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "erfan",
        "ebrahimi",
        "yeganehha",
        "filament",
        "multi-select",
        "select"
    ]
}
